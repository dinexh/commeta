{
  "name": "commeta-cli",
  "version": "1.0.1",
  "description": "A CLI tool that generates semantic git commit messages using AI/LLM services like Groq",
  "main": "dist/index.js",
  "bin": {
    "commeta": "dist/index.js"
  },
  "scripts": {
    "dev": "ts-node src/index.ts",
    "build": "tsc -p .",
    "start": "node dist/index.js"
  },
  "keywords": [
